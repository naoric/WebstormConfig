<templateSet group="Material UI">
  <template name="icn" value="import $NAME$ from '@material-ui/icons/$NAME$'" description="import svg icon" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="mat" value="import $COMP$ from '@material-ui/core/$COMP$'$END$" description="Import Material Component" toReformat="false" toShortenFQNames="true">
    <variable name="COMP" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="ims" value="import $NAME$ from '@material-ui/styles/$NAME$'" description="import material style" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="gi" value="&lt;Grid item$ATTR$&gt;&#10;  $END$&#10;&lt;/Grid&gt;" description="Grid item" toReformat="false" toShortenFQNames="true">
    <variable name="ATTR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="gc" value="&lt;Grid container$ATTR$&gt;&#10;  $END$&#10;&lt;/Grid&gt;" description="Grid container" toReformat="false" toShortenFQNames="true">
    <variable name="ATTR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
</templateSet>