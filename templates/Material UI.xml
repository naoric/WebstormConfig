<templateSet group="Material UI">
  <template name="icn" value="import $NAME$ from '@material-ui/icons/$NAME$'" description="import svg icon" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="mat" value="import $COMP$ from '@material-ui/core/$COMP$'$END$" description="Import Material Component" toReformat="false" toShortenFQNames="true">
    <variable name="COMP" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="ims" value="import $NAME$ from '@material-ui/styles/$NAME$'" description="import material style" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="gi" value="&lt;Grid item$ATTR$&gt;&#10;  $END$&#10;&lt;/Grid&gt;" description="Grid item" toReformat="false" toShortenFQNames="true">
    <variable name="ATTR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="gc" value="&lt;Grid container$ATTR$&gt;&#10;  $END$&#10;&lt;/Grid&gt;" description="Grid container" toReformat="false" toShortenFQNames="true">
    <variable name="ATTR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="withs" value="import createStyles from '@material-ui/styles/createStyles'&#10;import withStyles, { WithStyles } from '@material-ui/styles/withStyles'" description="import material style" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="mfc" value="import withStyles, { WithStyles } from '@material-ui/styles/withStyles'&#10;import createStyles from '@material-ui/styles/createStyles'&#10;import React from 'react'&#10;&#10;const styles = (theme: $TYPE$) =&gt; createStyles({})&#10;&#10;interface $TM_FILENAME_BASE$Props extends WithStyles&lt;typeof styles&gt; {&#10;  &#10;}&#10;&#10;function $TM_FILENAME_BASE$({ classes }: $TM_FILENAME_BASE$Props) {&#10; $END$&#10;}&#10;&#10;export default withStyles(styles)($TM_FILENAME_BASE$)" description="Material Function Component" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TM_FILENAME_BASE" expression="capitalize(camelCase(fileNameWithoutExtension()))" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>