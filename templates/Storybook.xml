<templateSet group="Storybook">
  <template name="ttl" value="export default { &#10;    title: '$TITLE$',&#10;    decorators: [withKnobs]&#10;}&#10;$END$" description="Export Title" toReformat="true" toShortenFQNames="true">
    <variable name="TITLE" expression="camelCase(fileNameWithoutExtension())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>